<div class="containerInfo1">
  <div class="row">
    <div class="col s8 containerI">
      <article>
        {{#if (lte animalObj.score 40)}}
          <h2>
            {{animalObj.name}}
            , you are a Duck!
          </h2>
          <img src="/assets/duck.jpg" alt="duck" class="result" />
        {{else if (and (gte animalObj.score 50) (lte animalObj.score 60))}}
          <h2>
            {{animalObj.name}}
            , you are Pitbull!
          </h2>
          <img src="/assets/pitbull.jpg" alt="pitbull" class="result" />
        {{else if (and (gte animalObj.score 70) (lte animalObj.score 80))}}
          <h2>
            {{animalObj.name}}
            , you are a Snake!
          </h2>
          <img src="/assets/snake.jpg" alt="snake" class="result" />
        {{else}}
          <h2>
            Sorry
            {{animalObj.name}}
            , you Tiger now!
          </h2>
          <img src="/assets/tiger.jpg" alt="tiger" class="result" />
        {{/if}}
      </article>
      <br />
      <br />
    </div>
  </div>
</div>
